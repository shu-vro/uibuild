{
    "projectId": "uibuild",
    "projectName": "uibuild",
    "settings": {
        "services": {
            "account": true,
            "avatars": true,
            "databases": true,
            "locale": true,
            "health": true,
            "storage": true,
            "teams": true,
            "users": true,
            "sites": true,
            "functions": true,
            "graphql": true,
            "messaging": true
        },
        "auth": {
            "methods": {
                "jwt": true,
                "phone": true,
                "invites": true,
                "anonymous": true,
                "email-otp": true,
                "magic-url": true,
                "email-password": true
            },
            "security": {
                "duration": 31536000,
                "limit": 0,
                "sessionsLimit": 10,
                "passwordHistory": 0,
                "passwordDictionary": false,
                "personalDataCheck": false,
                "sessionAlerts": false,
                "mockNumbers": []
            }
        }
    },
    "databases": [
        {
            "$id": "uibuild-db",
            "name": "uibuild-db",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "workspaceData",
            "$permissions": [
                "read(\"any\")",
                "create(\"users\")",
                "update(\"users\")",
                "delete(\"users\")"
            ],
            "databaseId": "uibuild-db",
            "name": "workspaceData",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "slug",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 100,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "name",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 100,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "description",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 500,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "belongsTo",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 20,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "currentVersion",
                    "type": "integer",
                    "required": false,
                    "array": false,
                    "min": 0,
                    "max": 10000,
                    "default": 0
                },
                {
                    "key": "totalVersions",
                    "type": "integer",
                    "required": false,
                    "array": false,
                    "min": 0,
                    "max": 10000,
                    "default": 0
                },
                {
                    "key": "versions",
                    "type": "relationship",
                    "required": false,
                    "array": false,
                    "relatedCollection": "allVersions",
                    "relationType": "oneToMany",
                    "twoWay": true,
                    "twoWayKey": "workspace",
                    "onDelete": "cascade",
                    "side": "parent"
                }
            ],
            "indexes": []
        },
        {
            "$id": "allVersions",
            "$permissions": [
                "read(\"any\")",
                "create(\"users\")",
                "update(\"users\")",
                "delete(\"users\")"
            ],
            "databaseId": "uibuild-db",
            "name": "allVersions",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "versionNum",
                    "type": "integer",
                    "required": true,
                    "array": false,
                    "min": 0,
                    "max": 10000,
                    "default": null
                },
                {
                    "key": "versionUrl",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "format": "url",
                    "default": null
                },
                {
                    "key": "public_id",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 100,
                    "default": null,
                    "encrypt": false
                },
                {
                    "key": "workspace",
                    "type": "relationship",
                    "required": false,
                    "array": false,
                    "relatedCollection": "workspaceData",
                    "relationType": "oneToMany",
                    "twoWay": true,
                    "twoWayKey": "versions",
                    "onDelete": "cascade",
                    "side": "child"
                }
            ],
            "indexes": []
        }
    ]
}